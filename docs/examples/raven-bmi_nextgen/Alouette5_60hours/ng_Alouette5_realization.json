{
    "global": {
      "formulations": [
        {
          "name": "bmi_c++",
          "params": {
            "model_type_name": "raven",
            "library_file": "{PATH}/bin/libravenbmi.so",
            "init_config": "{PATH}/docs/examples/raven-bmi_nextgen/Alouette5_60hours/Alouette5.yaml",
            "main_output_variable": "streamflow",
            "variables_names_map" : {
              "temp_ave": "TMP_2maboveground",
              "rainfall": "precip_rate"
            },
            "create_function": "bmi_model_create",
            "destroy_function_": "bmi_model_destroy",
            "uses_forcing_file": false
          }
        }
      ],
      "forcing": {
          "file_pattern": "ALUgauge_mm-h.csv",
          "path": "{PATH}/docs/examples/raven-bmi_nextgen/Alouette5_60hours/"
      }
    },
    "time": {
        "start_time": "2016-01-01 01:00:00",
        "end_time":   "2016-01-03 12:00:00",
        "output_interval": 3600
    },
    "catchments": {
        "cat-3": {
          "formulations": [
            {
              "name": "bmi_c++",
              "params": {
                "model_type_name": "raven",
                "library_file": "{PATH}/docs/examples/bin/libravenbmi.so",
                "init_config": "{PATH}/docs/examples/raven-bmi_nextgen/Alouette5_60hours/Alouette5.yaml",
                "main_output_variable": "streamflow",
                "variables_names_map" : {
                  "temp_ave": "TMP_2maboveground",
                  "rainfall": "precip_rate"
                },
                "create_function": "bmi_model_create",
                "destroy_function_": "bmi_model_destroy",
                "uses_forcing_file": false
              }
            }
          ],
          "forcing": {
              "path": "{PATH}/docs/examples/raven-bmi_nextgen/Alouette5_60hours/ALUgauge_mm-h.csv",
              "provider": "CsvPerFeature"
          }
        }
    }
}
